  <UsingTask TaskName="ssrsmsbuildtasks.RS2008R2.SetReportDataSource" AssemblyFile="$(SSRSMSBuildTasksPath)" />
  <UsingTask TaskName="ssrsmsbuildtasks.RS2008R2.GetFullURL" AssemblyFile="$(SSRSMSBuildTasksPath)" />
  <UsingTask TaskName="ssrsmsbuildtasks.RS2008R2.AddShareDataSets" AssemblyFile="$(SSRSMSBuildTasksPath)" />
  <UsingTask TaskName="ssrsmsbuildtasks.RS2008R2.SetReportDataSet" AssemblyFile="$(SSRSMSBuildTasksPath)" />
</Project>